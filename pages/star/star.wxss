/* pages/star/star.wxss */
.noLine{
  border: none !important;
}
.header{
  width: 100%;
  height: 624rpx;
  position: relative;
}
.greyBtn{
  background: #eee !important;
  color: #999 !important;
}
.navigation{
  width: 100%;
  height: 320rpx;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  box-sizing: border-box;
  padding-top: 169rpx;
  background: #fff;
}
.navigation_item{
  width: 100rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  font-size: 30rpx;
  position: relative;
}
.navigation_item:not(:last-child){
  margin-right: 70rpx;
}
.navigation_item image{
  width: 60rpx;
  height: auto;
  margin-bottom: 20rpx;
}
.header-bg{
  width: 100%;
  height: 305rpx;
  vertical-align: middle;
}
.userInfo{
  width:690rpx;
  height:250rpx;
  background:rgba(255,255,255,1);
  border-radius:20px;
  box-shadow:0px 0px 10px rgba(105,114,227,0.13);
  position: absolute;
  top: 140rpx;
  left: 50%;
  transform: translateX(-50%);
}
.user-logo{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  background: #fff;
  position: absolute;
  left: 50%;
  transform: translate(-50%,-50%)
}
.user-logo .navi{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.user-logo image{
  width:92%;
  height:92%;
  border-radius:50%;
  display:block;
  margin:0 auto;
  margin-top:6rpx;
}
.user-name{
  display: block;
  text-align: center;
  margin-top: 82rpx;
  color: rgb(51,51,51);
  line-height: 88rpx;
  font-size: 36rpx;
  font-weight: bold;
}
.signIn{
  width:180rpx;
  height:60rpx;
  background:rgba(0,26,136,1);
  border-radius:30rpx;
  text-align: center;
  font-size: 26rpx;
  line-height: 60rpx;
  color: #fff;
  position: relative;
}
.signInBox{
  width:180rpx;
  height:60rpx;
  display: block;
  margin: 0 auto;
  border: none;
  position: relative;
}
.signInBox text{
  position: absolute;
  font-size: 30rpx;
  top: 0;
  right: -30rpx;
  color: rgba(0,26,136,1);
  opacity: 1;
}
.user-level{
  font-size: 28rpx;
  color: #666;
  text-align: center;
  position: absolute;
  left: 50%;
  top: 60%;
  transform: translateX(-50%);
  bottom: 61rpx;
}
.user-level image{
  width: 22rpx;
  margin-right: 10rpx;
}
.message{
  width: 50rpx;
  height: 50rpx;
  right: 46rpx;
  top: 43rpx;
  position: absolute;
}
.message image{
  width: 100%;
  height: 100%;
}
.message .tip1{
  width: 32rpx;
  height: 32rpx;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  line-height: 32rpx;
  background: #F52849;
  border-radius: 50%;
  position: absolute;
  left: 27rpx;
  top: 10rpx;
  transform: translateY(-40%)
}

.blank{
  height: 21rpx;
  width: 100%;
  background: #f5f7fd;
}

.userInfoBox image{
  width: 45rpx;
  height: auto;
}
.rowSet{
  height: 120rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding-left: 30rpx;
  line-height: 120rpx;
  align-items: center;
  position: relative;
  background: #fff;
  box-sizing: border-box;
}

.titleSecond{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 92%;
  vertical-align: middle;
}
.titleSecond text{
  color: #333;
  font-size: 30rpx;
  }
.titleSecond text:nth-child(2){
  margin-right: 74rpx;
  color: #999;
}
.nextImage{
  width: 13rpx !important;
  margin-left: 30rpx;
}
.move{
  position: absolute;
  height: 100%;
  right: 31rpx;
  top: -3.7rpx;
}
.tip{
  width: 18rpx;
  height: 18rpx;
  background: #F52849;
  border-radius: 50%;
  position: absolute;
  top: 54%;
  transform: translateY(-50%);
  right: 50rpx;
}
.tabTip{
  width: 43rpx;
  height: 36rpx;
  background: #F52849;
  border-radius: 18rpx;
  position: absolute;
  text-align: center;
  color: #ffff;
  line-height: 36rpx;
  right: 0;
  top: 0;
  transform: translateY(-20%);
}
.tipOne{
  width: 32rpx;
  height: 32rpx;
  background: #F52849;
  border-radius: 50%;
  position: absolute;
  top: 8%;
  transform: translateY(-50%);
  right: 8%;
  text-align: center;
  line-height: 32rpx;
  color: #fff;
  font-size: 24rpx;
}
.blank2{
  height: 37rpx;
  width: 100%;
  background: #f5f7fd;
  border-bottom: 1px solid #f5f5f5;
}